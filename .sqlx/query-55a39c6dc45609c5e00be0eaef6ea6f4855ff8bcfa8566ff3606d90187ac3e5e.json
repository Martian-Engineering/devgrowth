{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT\n            c.collection_id,\n            c.owner_id,\n            c.name,\n            c.description,\n            c.is_default,\n            c.created_at,\n            c.updated_at,\n            COUNT(cr.repository_id) AS repository_count\n        FROM collection c\n        LEFT JOIN collection_repository cr ON c.collection_id = cr.collection_id\n        WHERE c.collection_id = $1\n        GROUP BY c.collection_id\n        ORDER BY c.created_at DESC\n        ",
